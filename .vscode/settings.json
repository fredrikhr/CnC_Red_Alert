{
  "files.encoding": "windows1252",
  "files.associations": {
    "*.ASM": "masm",
    "*.I": "masm",
    "*.INC": "masm",
    "*.16": "masm"
  },
  "cmake.buildDirectory": "${workspaceFolder}/OBJ/${generator}/${buildType}",
  "cmake.installPrefix": "${workspaceFolder}/BIN/${buildType}",
  "[cmake]": {
    "files.encoding": "utf8"
  },
  "[cmake-cache]": {
    "files.encoding": "utf8"
  },
  "[xml]": {
    "editor.autoClosingBrackets": "never",
    "files.trimFinalNewlines": true,
    "files.encoding": "utf8"
  },
  "[json]": {
    "files.encoding": "utf8",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[editorconfig]": {
    "files.encoding": "utf8"
  },
  "[gitignore]": {
    "files.encoding": "utf8"
  }
}
