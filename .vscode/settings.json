{
  "files.encoding": "windows1252",
  "files.associations": {
    "*.mak": "makefile",
    "*.resx": "xml",
    "*.*proj": "xml",
    "*.props": "xml",
    "*.ruleset": "xml",
    "*.slnx": "xml",
    "*.ASM": "masm",
    "*.I": "masm",
    "*.INC": "masm",
    "*.16": "masm",
    "*.C": "c",
    "*.CPP": "cpp",
    "**/CODE/ENG/*.TXT": "x-cnc-redalert-eng",
    "**/CODE/ENG/*.BAK": "x-cnc-redalert-eng",
    "**/CODE/ENG/*/*.TXT": "x-cnc-redalert-eng",
    "**/CODE/ENG/*/*.BAK": "x-cnc-redalert-eng"
  },
  "cmake.buildDirectory": "${workspaceFolder}/BLD/${generator}",
  "cmake.installPrefix": "${workspaceFolder}/BIN/${buildType}",
  "cmake.platform": "Win32",
  "cmake.preferredGenerators": [
    "Visual Studio 18 2026",
    "Visual Studio 17 2022",
    "Visual Studio 16 2019",
    "Visual Studio 15 2017",
    "Visual Studio 14 2015",
  ],
  "[x-cnc-redalert-eng]": {
    "files.encoding": "cp850"
  },
  "[cmake]": {
    "files.encoding": "utf8"
  },
  "[cmake-cache]": {
    "files.encoding": "utf8"
  },
  "[xml]": {
    "editor.autoClosingBrackets": "never",
    "files.trimFinalNewlines": true,
    "files.encoding": "utf8"
  },
  "[json]": {
    "files.encoding": "utf8",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[editorconfig]": {
    "files.encoding": "utf8"
  },
  "[gitignore]": {
    "files.encoding": "utf8"
  }
}
