{
  "files.encoding": "windows1252",
  "files.associations": {
    "*.mak": "makefile",
    "*.resx": "xml",
    "*.*proj": "xml",
    "*.props": "xml",
    "*.ruleset": "xml",
    "*.slnx": "xml",
    "*.ASM": "masm",
    "*.I": "masm",
    "*.INC": "masm",
    "*.16": "masm",
    "*.C": "c",
    "*.CPP": "cpp",
  },
  "cmake.buildDirectory": "${workspaceFolder}/OBJ/${generator}/${buildType}",
  "cmake.installPrefix": "${workspaceFolder}/BIN/${buildType}",
  "[cmake]": {
    "files.encoding": "utf8"
  },
  "[cmake-cache]": {
    "files.encoding": "utf8"
  },
  "[xml]": {
    "editor.autoClosingBrackets": "never",
    "files.trimFinalNewlines": true,
    "files.encoding": "utf8"
  },
  "[json]": {
    "files.encoding": "utf8",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[editorconfig]": {
    "files.encoding": "utf8"
  },
  "[gitignore]": {
    "files.encoding": "utf8"
  }
}
