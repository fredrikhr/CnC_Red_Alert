{
  "problemMatcher": [
    {
      "owner": "visual-studio-cmake",
      "pattern": [
        {
          "regexp": "^(.*)\\s*\\((\\d+)\\)\\s*:\\s*(warning|error)\\s*([^:]*):\\s*(.*)$",
          "file": 1,
          "line": 2,
          "severity": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "visual-studio-cmake-fatal",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*)\\s*\\((\\d+)\\)\\s*:\\s*fatal\\s*(error)\\s*([^:]*):\\s*(.*)$",
          "file": 1,
          "line": 2,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
